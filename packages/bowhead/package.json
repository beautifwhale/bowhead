{
  "name": "@mortond/bowhead",
  "version": "0.2.16",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.42",
    "@mortond/eslint-config": "^0.2.12",
    "@stripe/stripe-js": "^1.8.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "firebase": "^7.19.0",
    "firebase-admin": "^9.0.0",
    "firebase-tools": "^8.7.0",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "netlify-cli": "^2.59.0",
    "netlify-lambda": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-redux-firebase": "^3.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-firestore": "^0.13.0",
    "redux-thunk": "^2.3.0",
    "shelljs": "^0.8.4",
    "stripe": "^8.84.0",
    "webpack-node-externals": "^2.5.1"
  },
  "scripts": {
    "start": "netlify dev -c 'yarn dev'",
    "dev": "npm-run-all -p --race stripe:webhooks firebase:emulator client:start",
    "stripe:webhooks": "stripe listen --forward-to localhost:8888/.netlify/functions/webhook-stripe",
    "firebase:emulator": "firebase emulators:start",
    "client:start": "react-scripts start",
    "build": "react-scripts build && yarn build:functions",
    "build:netlify": "yarn build && yarn deploy:firebase:rules",
    "build:functions": "netlify-lambda build src/functions --config ./config/webpack.functions.js",
    "deploy:firebase:rules": "node scripts/deploy-firebase-rules.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "@mortond/eslint-config"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}