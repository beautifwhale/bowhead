{
  "package": {
    "dependencies": {
      "@material-ui/core": "^4.9.1",
      "@material-ui/icons": "^4.9.1",
      "@material-ui/lab": "^4.0.0-alpha.42",
      "@mortond/bowhead": "^0.0.3",
      "@mortond/eslint-config": "^0.2.13",
      "@stripe/stripe-js": "^1.8.0",
      "dotenv": "^8.2.0",
      "eslint": "^7.7.0",
      "firebase": "^7.17.2",
      "firebase-admin": "^9.0.0",
      "firebase-tools": "^8.7.0",
      "materialize-css": "^1.0.0",
      "moment": "^2.24.0",
      "netlify-cli": "^2.59.1",
      "netlify-lambda": "^2.0.1",
      "npm-run-all": "^4.1.5",
      "react": "^16.12.0",
      "react-cookie": "^4.0.3",
      "react-dom": "^16.12.0",
      "react-redux": "^7.1.3",
      "react-redux-firebase": "^3.1.0",
      "react-router-dom": "^5.1.2",
      "react-scripts": "^3.4.3",
      "redux": "^4.0.5",
      "redux-devtools-extension": "^2.13.8",
      "redux-firestore": "^0.11.0",
      "redux-thunk": "^2.3.0",
      "shelljs": "^0.8.4",
      "stripe": "^8.84.0",
      "webpack-node-externals": "^2.5.1"
    },
    "scripts": {
      "start": "netlify dev -c 'yarn dev'",
      "dev": "npm-run-all -p --race stripe:webhooks firebase:emulator client:start",
      "stripe:webhooks": "stripe listen --forward-to localhost:8888/.netlify/functions/webhook-stripe",
      "firebase:emulator": "firebase emulators:start --only firestore",
      "client:start": "react-scripts start",
      "build": "react-scripts build && yarn build:functions",
      "build:functions": "netlify-lambda build src/functions --config ./config/webpack.functions.js",
      "build:netlify": "yarn build && yarn deploy:firebase:rules",
      "deploy:firebase:rules": "node scripts/deploy-firebase-rules.js",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "clean": "rm -rf node_modules build functions",
      "lint": "eslint ."
    },
    "eslintConfig": {
      "extends": [
        "@mortond/eslint-config"
      ]
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }
}